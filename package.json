{
  "name": "portfolio",
  "version": "0.1.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "proxy": "http://localhost:3000",
  "scripts": {
    "start-js": "react-scripts start",
    "start": "babel-node tools/server.js",
    "build-js": "react-scripts build",
    "build": "babel-node tools/build.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHTML.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "post-build": "babel-node tools/distServer.js"
  },
  "author": "Omotola Shogunle",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-loader": "6.2.4",
