    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "nodemailer": "^4.6.8",
    "nodemailer-mailgun-transport": "^1.4.0",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
